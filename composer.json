{
    "name": "buyersclub/laravel-eloquent-model-interface",
    "description": "Polyfill for the \"missing\" Laravel Eloquent model interface.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Johan Rosenson",
            "email": "johan@buyersclub.se"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Buyersclub\\Laravel\\Eloquent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Buyersclub\\Laravel\\Eloquent\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
